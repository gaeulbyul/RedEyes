{
  "incremental": true,
  "typescript": {
  },
  "json": {
  },
  "markdown": {
  },
  "toml": {
  },
  "prettier": {
    "printWidth": 100,
    "trailingComma": "es5",
    "singleQuote": true,
    "semi": false,
    "arrowParens": "avoid"
  },
  "includes": ["**/*.{svelte,vue,ts,tsx,js,jsx,cjs,mjs,json,md,toml}"],
  "excludes": [
    "**/.yarn",
    "**/node_modules",
    "**/*-lock.json"
  ],
  "plugins": [
    "https://plugins.dprint.dev/typescript-0.61.0.wasm",
    "https://plugins.dprint.dev/json-0.14.0.wasm",
    "https://plugins.dprint.dev/markdown-0.12.0.wasm",
    "https://plugins.dprint.dev/toml-0.5.3.wasm",
    "https://plugins.dprint.dev/prettier-0.4.0.exe-plugin@2a38bff5dff736cb53195a93d6a15a6281580205dfb0552ca0562bac8b3fc065"
  ]
}
