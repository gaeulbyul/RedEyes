{
  "incremental": true,
  "useTabs": false,
  "lineWidth": 100,
  "indentWidth": 2,
  "newLineKind": "lf",
  "typescript": {
    "semiColons": "asi",
    "quoteStyle": "preferSingle",
    "useBraces": "always",
    "bracePosition": "sameLine",
    "trailingCommas": "onlyMultiLine",
    "arrowFunction.useParentheses": "preferNone",
    "jsx.quoteStyle": "preferDouble",
    "jsx.multiLineParens": "prefer",
    "typeLiteral.separatorKind.multiLine": "semiColon",
    "typeLiteral.separatorKind.singleLine": "comma"
  },
  "json": {
    "associations": [
      ".swcrc"
    ]
  },
  "markdown": {
  },
  "toml": {
  },
  "includes": [
    "src/**/*.{ts,tsx,js,jsx,cjs,mjs,json,md,toml}",
    "/*.{md,json,js}"
  ],
  "excludes": [
    "**/.yarn",
    "**/node_modules",
    "*vendor*",
    "**/*-lock.json"
  ],
  "plugins": [
    "https://plugins.dprint.dev/typescript-0.61.0.wasm",
    "https://plugins.dprint.dev/json-0.14.0.wasm",
    "https://plugins.dprint.dev/markdown-0.12.0.wasm",
    "https://plugins.dprint.dev/toml-0.5.3.wasm"
  ]
}
